#include <stdio.h>
void shell(int arr[],int n){
	for(int gap=n/2;gap>0;gap/=2){
		//TODO
		for(int i=gap;i<n;i++){
			//TODO
			int temp=arr[i];
			int j;
			for(int j=i;j>=gap&&arr[j-gap]>arr[j];j-=gap){
				arr[j]=arr[j-gap];
				
				//TODO
			}
			arr[j]=temp;
		}
	}
}


int main()
{
	int n;
	int arr[n];
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		//TODO
		scanf("%d",&arr[i]);
	}
	shell(arr,n);
	for(int i=0;i<n;i++){
		//TODO
		printf("%d",arr[i]);
	}
		return 0;
	}



