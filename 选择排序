#include <stdio.h>
void swap(int arr[],int n){
	for(int i=0;i<n;i++){
		//TODO
		int temp;
		temp=arr[i];
		arr[i]=arr[n-i-1];
		arr[n-i-1]=temp;
	}
}
void select(int arr[],int n){
	for(int i=0;i<n;i++){
		//TODO
		int min=i;
		for(int j=i+1;j<n;j++){
			//TODO
			if(arr[j]<arr[min]){
				//TODO
				min=j;
			}
		}
		swap(&arr[min],&arr[i]);
	}
}

int main()
{
	int n;
	int arr[n];
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		//TODO
		scanf("%d",&arr[i]);
	}
	select(arr,n);
	for(int i=0;i<n;i++){
		//TODO
		printf("%d",arr[i]);
	}
	return 0;
}



